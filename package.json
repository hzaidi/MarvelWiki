{
  "name": "marvel-wiki",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development react-scripts start",
    "build": "yarn build:dl && cross-env NODE_ENV=production react-scripts build && yarn buildLambda",
    "buildLambda": "netlify-lambda build ./lambdaFunctions",
    "build:dl": "del ./build",
    "serve:fn": "netlify-lambda serve ./lambdaFunctions",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.0.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "cross-env": "^5.1.5",
    "debounce": "^1.1.0",
    "del-cli": "^1.1.0",
    "netlify-lambda": "^0.4.0",
    "npm-run-all": "^4.1.2",
    "plop": "^2.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.2.0",
    "yarn": "^1.6.0"
  }
}
